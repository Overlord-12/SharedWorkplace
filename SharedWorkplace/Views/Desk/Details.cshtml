@model IEnumerable<SharedWorkplace.Models.Device>

@{
    ViewData["Title"] = "DeviceList";
}
<h1>DeskList</h1>
@*@if (User.IsInRole("admin"))
    {
    <p>
        <a asp-action="Create">Create New</a>
    </p>
    }*@
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DeviceName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.DeviceName)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.DeviceName">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>